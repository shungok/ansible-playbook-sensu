{
  "rabbitmq": {
    "ssl": {
      "private_key_file": "/etc/sensu/ssl/key.pem",
      "cert_chain_file": "/etc/sensu/ssl/cert.pem"
    },
    "host": "{{ rabbitmq_ip }}",
    "port": {{ rabbitmq_port }},
    "user": "{{ rabbitmq_sensu_user }}",
    "password": "{{ rabbitmq_sensu_pass }}",
    "vhost": "{{ rabbitmq_sensu_vhost }}"
  }
}
